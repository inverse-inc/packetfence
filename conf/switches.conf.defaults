[default]
type=PacketFence::Standard
description=Switches Default Values
vlans = 1,2,3,4,5
normalVlan = 1
registrationVlan = 2
isolationVlan = 3
voiceVlan = 5
inlineVlan = 6
REJECTVlan = -1
inlineTrigger = 
normalRole = 
registrationRole =
isolationRole =
macDetectionRole =
voiceRole = voice
inlineRole = inline
VoIPEnabled = N
cliAccess = N
VlanMap = Y
RoleMap = N
VpnMap = N
ExternalPortalEnforcement = N

mode = production
macSearchesMaxNb = 30
macSearchesSleepInterval = 2
uplink = dynamic
pushACLs = N

#
# Command Line Interface 
#
# cliTransport could be: Telnet, SSH or Serial
cliTransport = Telnet
cliUser = 
cliPwd = 
cliEnablePwd = 
#
# SNMP section
#

# PacketFence -> Switch
SNMPUseConnector=Y
SNMPVersion = 1
SNMPCommunityRead = public
SNMPCommunityWrite = private
#SNMPEngineID = 0000000000000
#SNMPUserNameRead = readUser
#SNMPAuthProtocolRead = MD5
#SNMPAuthPasswordRead = authpwdread
#SNMPPrivProtocolRead = DES 
#SNMPPrivPasswordRead = privpwdread
#SNMPUserNameWrite = writeUser
#SNMPAuthProtocolWrite = MD5
#SNMPAuthPasswordWrite = authpwdwrite
#SNMPPrivProtocolWrite = DES 
#SNMPPrivPasswordWrite = privpwdwrite

# Switch -> PacketFence
SNMPVersionTrap = 1
SNMPCommunityTrap = public
#SNMPAuthProtocolTrap = MD5
#SNMPAuthPasswordTrap = authpwdread
#SNMPPrivProtocolTrap = DES
#SNMPPrivPasswordTrap = privpwdread

#
# Web Services Interface 
#
# wsTransport could be: http or https
wsTransport = http
wsUser = 
wsPwd = 
#
# RADIUS NAS Client config
#
# RADIUS shared secret with switch
radiusSecret=

VoIPCDPDetect=Y
VoIPDHCPDetect=Y
AccessListMap=N
UrlMap=N
useCoA=Y
radiusDeauthUseConnector=Y
VoIPLLDPDetect=Y
PostMfaValidation=N

# Push ACLs
PushACLs=N

# Downloadable ACLs
DownloadableACLsLimit=384
UseDownloadableACLs=N
ACLsLimit=20

deauthOnPrevious=N
