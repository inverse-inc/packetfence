[Accounting::Bandwidth]
type=sql
description=Total accounting bandwidth
columns=time_bucket,bytes_in,bytes_out,bytes_total
sql= <<EOT
BEGIN NOT ATOMIC
  SET @TENANT_ID = ?, @START_DATE = ?, @END_DATE = ?;
  SELECT
    time_bucket,
    SUM(in_bytes) AS bytes_in,
    SUM(out_bytes) AS bytes_out,
    SUM(total_bytes) AS bytes_total
  FROM (
    SELECT
      time_bucket,
      in_bytes,
      out_bytes,
      total_bytes
    FROM bandwidth_accounting
    WHERE tenant_id = @TENANT_ID
      AND time_bucket >= @START_DATE
      AND time_bucket <= @END_DATE
    UNION ALL
    SELECT
      time_bucket,
      in_bytes,
      out_bytes,
      total_bytes
    FROM bandwidth_accounting_history
    WHERE tenant_id = @TENANT_ID
      AND time_bucket >= @START_DATE
      AND time_bucket <= @END_DATE
  ) `total`
  GROUP BY time_bucket
  ORDER BY time_bucket DESC
  LIMIT ?;
END;
EOT
bindings=tenant_id,start_date,end_date,limit
has_date_range=enabled
has_limit=enabled
cursor_type=none
date_limit=24h
default_limit=100
charts=scatter|time_bucket:bytes_in:bytes_out:bytes_total

[Accounting::Node]
type=sql
description=Top node accounting bandwidth
columns=mac,bytes_in,bytes_out,bytes
sql= <<EOT
BEGIN NOT ATOMIC
  SET @TENANT_ID = ?, @START_DATE = ?, @END_DATE = ?, @CURSOR_0 = ?, @CURSOR_1 = ?;
  SELECT * FROM (
      SELECT
        mac,
        SUM(in_bytes) AS bytes_in,
        SUM(out_bytes) AS bytes_out,
        SUM(total_bytes) AS bytes
      FROM (
        SELECT
          mac,
          in_bytes,
          out_bytes,
          total_bytes
        FROM bandwidth_accounting
        WHERE tenant_id = @TENANT_ID
          AND time_bucket >= @START_DATE
          AND time_bucket <= @END_DATE
        UNION ALL
        SELECT
          mac,
          in_bytes,
          out_bytes,
          total_bytes
        FROM bandwidth_accounting_history
        WHERE tenant_id = @TENANT_ID
          AND time_bucket >= @START_DATE
          AND time_bucket <= @END_DATE
      ) `total`
      GROUP BY mac
      ORDER BY bytes DESC, mac DESC
 ) as x
 WHERE
     (bytes, mac) <= (@CURSOR_0, @CURSOR_1)
  LIMIT ?;
END;
EOT
bindings=tenant_id,start_date,end_date,cursor.0,cursor.1,limit
has_date_range=enabled
has_limit=enabled
cursor_type=multi_field
cursor_field=bytes,mac
cursor_default=18446744073709551615,ff:ff:ff:ff:ff:ff
date_limit=24h
default_limit=25
node_fields=mac

[Accounting::Operating System Class]
type=sql
description=Top Fingerbank operating system class accounting bandwidth
columns=dhcp_fingerprint,bytes_in,bytes_out,bytes
sql= <<EOT
BEGIN NOT ATOMIC
  SET @TENANT_ID = ?, @START_DATE = ?, @END_DATE = ?, @CURSOR = ?;
  SELECT
    IFNULL(device_class, 'Unknown Fingerprint') AS dhcp_fingerprint,
    SUM(in_bytes) AS bytes_in,
    SUM(out_bytes) AS bytes_out,
    SUM(in_bytes) + SUM(out_bytes) AS bytes
  FROM (
    SELECT
      tenant_id,
      mac,
      in_bytes,
      out_bytes
    FROM bandwidth_accounting
    WHERE tenant_id = @TENANT_ID
      AND time_bucket >= @START_DATE
      AND time_bucket <= @END_DATE
    UNION ALL
    SELECT
      tenant_id,
      mac,
      in_bytes,
      out_bytes
    FROM bandwidth_accounting_history
    WHERE tenant_id = @TENANT_ID
      AND time_bucket >= @START_DATE
      AND time_bucket <= @END_DATE
  ) `total`
  LEFT JOIN node USING (tenant_id, mac)
  WHERE device_class >= @CURSOR
  GROUP BY device_class
  ORDER BY bytes DESC
  LIMIT ?;
END;
EOT
bindings=tenant_id,start_date,end_date,cursor,limit
has_date_range=enabled
has_limit=enabled
cursor_type=field
cursor_field=device_class
date_limit=24h
default_limit=25

