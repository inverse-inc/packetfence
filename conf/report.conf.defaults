[RADIUS Authentication::Top Failures::By MAC]
type=sql
description=Top authentication failures by MAC
columns=mac,count,percent
sql= <<EOT
BEGIN NOT ATOMIC
  SET @TENANT_ID = ?, @START_DATE = ?, @END_DATE = ?;
  SELECT
    `mac`,
    COUNT(1) AS `count`,
    SUM(100) / `total` AS `percent`
  FROM `radius_audit_log`
  CROSS JOIN (
    SELECT
      COUNT(1) AS `total`
    FROM `radius_audit_log`
    WHERE `tenant_id` = @TENANT_ID
      AND `auth_status` = 'Reject'
      AND `created_at` BETWEEN @START_DATE AND @END_DATE
  ) `x`
  WHERE `tenant_id` = @TENANT_ID
    AND `auth_status` = 'Reject'
    AND `created_at` BETWEEN @START_DATE AND @END_DATE
    AND `mac` IS NOT NULL
  GROUP BY 1
  ORDER BY `percent` DESC
  LIMIT ?;
END;
EOT
