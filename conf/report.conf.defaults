    ROUND(COUNT(1) / @TOTAL * 100, 1) AS percent
  FROM locationlog
  INNER JOIN node
    ON node.mac = locationlog.mac AND node.tenant_id = locationlog.tenant_id
  WHERE locationlog.tenant_id = @TENANT_ID
    AND node.status = 'reg'
  GROUP BY role
  ORDER BY nodes DESC;
END;
EOT
bindings=tenant_id
has_limit=disabled
cursor_type=none
charts=pie|role:nodes,bar|role:nodes
role_fields=role

