[SSID::Active]
type=sql
description=Active SSID's
columns=ssid,nodes,percent
sql= <<EOT
BEGIN NOT ATOMIC
  SET @TENANT_ID = ?;
  SET @TOTAL = (
    SELECT
      COUNT(1)
    FROM locationlog
    INNER JOIN node
      ON node.mac = locationlog.mac AND node.tenant_id = locationlog.tenant_id
    INNER JOIN ip4log
      ON ip4log.mac = node.mac AND ip4log.tenant_id = node.tenant_id
    WHERE locationlog.tenant_id = @TENANT_ID
      AND ssid != ""
      AND (ip4log.end_time = 0 OR ip4log.end_time > NOW())
  );
  SELECT
    ssid,
    COUNT(1) AS nodes,
    ROUND( COUNT(1) / @TOTAL * 100, 1) AS percent
  FROM locationlog
  INNER JOIN node
    ON node.mac = locationlog.mac AND node.tenant_id = locationlog.tenant_id
  INNER JOIN ip4log
    ON ip4log.mac = node.mac AND ip4log.tenant_id = node.tenant_id
  WHERE locationlog.tenant_id = @TENANT_ID
    AND ssid != ""
    AND (ip4log.end_time = 0 OR ip4log.end_time > NOW())
  GROUP BY ssid
  ORDER BY percent DESC;
END;
EOT
bindings=tenant_id
has_limit=disabled
cursor_type=none
charts=pie|ssid:nodes,bar|ssid:nodes

[SSID::All]
type=sql
description=All SSID's
columns=ssid,nodes,percent
sql= <<EOT
BEGIN NOT ATOMIC
  SET @TENANT_ID = ?, @START_DATE = ?, @END_DATE = ?;
  SET @TOTAL = (
    SELECT
      COUNT(DISTINCT locationlog.mac)
    FROM locationlog
    INNER JOIN node
      ON node.mac = locationlog.mac AND node.tenant_id = locationlog.tenant_id
    INNER JOIN ip4log
      ON ip4log.mac = node.mac AND ip4log.tenant_id = node.tenant_id
    WHERE locationlog.tenant_id = @TENANT_ID
      AND ssid != ""
      AND ip4log.start_time BETWEEN @START_DATE AND @END_DATE
  );
  SELECT
    ssid,
    COUNT(DISTINCT locationlog.mac) AS nodes,
    ROUND( COUNT(DISTINCT locationlog.mac) / @TOTAL * 100, 1) AS percent
  FROM locationlog
  INNER JOIN node
    ON node.mac = locationlog.mac AND node.tenant_id = locationlog.tenant_id
  INNER JOIN ip4log
    ON ip4log.mac = node.mac AND ip4log.tenant_id = node.tenant_id
  WHERE locationlog.tenant_id = @TENANT_ID
  AND ssid != ""
  AND ip4log.end_time BETWEEN @START_DATE AND @END_DATE
  GROUP BY ssid
  ORDER BY percent DESC;
END;
EOT
bindings=tenant_id,start_date,end_date
has_date_range=enabled
has_limit=disabled
cursor_type=none
charts=pie|ssid:nodes,bar|ssid:nodes

[Top Sponsors::All]
type=sql
description=Top sponsors
columns=contact_info,sponsor
sql= <<EOT
BEGIN NOT ATOMIC
  SET @TENANT_ID = ?;
  SELECT
    contact_info,
    COUNT(contact_info) AS sponsor
  FROM activation
  WHERE tenant_id = @TENANT_ID
    AND type = 'sponsor'
  GROUP BY contact_info
  LIMIT ?;
END;
EOT
