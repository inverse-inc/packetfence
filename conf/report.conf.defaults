[Authentication::Top Failures::Connection Profile to Source]
type=sql
description=All failed authentications of sources vs profiles
columns=profile,source,count,percent
sql= <<EOT
BEGIN NOT ATOMIC
  SET @START_DATE = ?, @END_DATE = ?;
  SET @TOTAL = (
    SELECT
      COUNT(1) FROM auth_log
     WHERE status='failed'
      AND completed_at >= @START_DATE
      AND completed_at <= @END_DATE
  );
  SELECT
    profile,
    source,
    COUNT(1) AS count,
    ROUND(COUNT(1) / @TOTAL * 100, 1) AS percent
  FROM auth_log
  WHERE status='failed'
      AND completed_at >= @START_DATE
      AND completed_at <= @END_DATE
  GROUP BY profile, source
  ORDER BY percent DESC;
END;
EOT
bindings=start_date,end_date
has_date_range=enabled
has_limit=disabled
cursor_type=none
charts=parallel|profile:source:count

