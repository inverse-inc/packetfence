[storage configfiles]
storage=file

[storage configfiles l1_cache]
storage=memcached

[storage configfilesdata]
storage=file

[storage configfilesdata l1_cache]
storage=memcached

[storage httpd.admin]
storage=file
expires_in=1d

[storage httpd.admin l1_cache]
storage=memcached
expires_on_backend=1

[storage httpd.portal]
storage=file
expires_in=10m

[storage httpd.portal l1_cache]
storage=memcached
expires_on_backend=1

[storage pfdns]
storage=file

[storage pfdns l1_cache]
storage=memcached

[storage memcached]
driver=Memcached
servers=127.0.0.1:11211
global=1
compress_threshold=10000

[storage file]
driver=File
root_dir=/usr/local/pf/var/cache

[dbi]
db=pf
user=pf
pass=packet
port=3306
host=localhost
