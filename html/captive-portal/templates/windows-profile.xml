<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<!-- Generated by the iPhone Configuration Utility /-->
<plist version="1.0">
<dict>
        <key>PayloadContent</key>
        <array>
                <dict>
                        [% IF provisioner.eap_type != "" and provisioner.security_type != "none" %]
                        <key>AutoJoin</key>
                        <true/>
                        <key>EAPClientConfiguration</key>
                        <dict>
                                <key>AcceptEAPTypes</key>
                                <array>
                                        <integer>[% provisioner.eap_type %]</integer>
                                </array>
                                <key>UserName</key>
                                <string>[% username %]</string>
                        </dict>
                        [% END %]
                        <key>EncryptionType</key>
                        <string>[% provisioner.security_type %]</string>
                        <key>HIDDEN_NETWORK</key>
                        [% IF provisioner.broadcast %]
                        <false/>
                        [% ELSE %]
                        <true/>
                        [% END %]
                        <key>SSID_STR</key>
                        <string>[% provisioner.ssid %]</string>
                        [% IF provisioner.eap_type.undefined %]
                        <key>Password</key>
                        <string>[% passcode %]</string>
                        [% END %]
                </dict>
                [% IF provisioner.eap_type.defined and provisioner.eap_type == "13" %]
                <dict>
                        <key>PayloadCertificateFileName</key>
                        <string>[% cert_cn %]</string>
                        <key>PayloadContent</key>
                        <string>[% cert_content %]</string>
                        <key>PayloadDisplayName</key>
                        <string>[% cert_cn %]</string>
                        <key>PayloadIdentifier</key>
                        <string>[% reverse_fqdn %]</string>
                        <key>PayloadType</key>
                        <string>com.apple.security.pkcs12</string>
                </dict>
                [% END %]
                [% IF provisioner.eap_type.defined and provisioner.eap_type == "13" %]
                <dict>
                        <key>PayloadCertificateFileName</key>
                        <string>[% ca_cn %]</string>
                        <key>PayloadContent</key>
                        <string>[% ca_content %]</string>
                        <key>PayloadDisplayName</key>
                        <string>[% ca_cn %]</string>
                        <key>PayloadIdentifier</key>
                        <string>[% reverse_fqdn %]</string>
                        <key>PayloadType</key>
                        <string>com.apple.security.root</string>
                </dict>
                [% END %]
        </array>
</dict>
</plist>
